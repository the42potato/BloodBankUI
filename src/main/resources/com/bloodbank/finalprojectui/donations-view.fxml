<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.bloodbank.finalprojectui.DonationsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <GridPane alignment="CENTER" hgap="5" vgap="10">
        <TableView GridPane.columnIndex="0" GridPane.rowIndex="0"
               GridPane.columnSpan="1" GridPane.rowSpan="15" fx:id="donationsTable">

        </TableView>
        <Button text="Back" onAction="#onBackButtonClick"
                GridPane.columnIndex="1" GridPane.rowIndex="0"
                GridPane.columnSpan="1" GridPane.rowSpan="1"/>
    </GridPane>
</VBox>
